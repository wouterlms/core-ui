{
  "name": "@wouterlms/ui",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "publish-test": "npm run build && cp package.json dist && cd dist && rm vite.config.d.ts",
    "publish-lib": "npm run build && cp package.json dist && cd dist && rm vite.config.d.ts && npm publish && cd .. && rm -r dist"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./style.css": {
      "import": "./style.css"
    }
  },
  "main": "index.js",
  "module": "index.js",
  "types": "types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@wouterlms/composables": "^0.1.8",
    "focus-trap": "^6.7.2",
    "focus-trap-vue": "^3.2.1",
    "sass": "^1.49.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/tailwindcss": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-putout": "^12.9.1",
    "eslint-plugin-vue": "^8.4.0",
    "postcss": "^8.4.5",
    "putout": "^23.5.0",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vue-tsc": "^0.29.8",
    "eslint": "^8.8.0"
  }
}
